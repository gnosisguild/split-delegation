{
  "name": "delegate-registry-api",
  "version": "1.0.0",
  "license": "LGPL-3.0+",
  "description": "For computing vote weight for voters in Snapshot proposals",
  "repository": "https://github.com/gnosis/delegate-registry",
  "scripts": {
    "test": "prisma generate && jest",
    "start": "vercel dev ../..",
    "deploy": "vercel deploy",
    "lint": "tsc --noEmit && eslint 'src/**/*.ts' --max-warnings 0 --fix",
    "lint:check": "tsc --noEmit && eslint  'src/**/*.ts'",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json,css,md,graphql}\" --write",
    "migrate:dev": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "sync": "prisma generate && ts-node src/commands sync",
    "trim": "prisma generate && ts-node src/commands trim",
    "postinstall": "prisma generate",
    "doStuff": "ts-node src/commands/doStuff.ts"
  },
  "dependencies": {
    "@prisma/client": "5.12.1",
    "@snapshot-labs/snapshot.js": "^0.11.18",
    "@vercel/edge": "^1.1.1",
    "@vercel/node": "3.0.28",
    "async": "^3.2.5",
    "cmd-ts": "^0.13.0",
    "dotenv": "^16.4.5",
    "prisma": "5.12.1",
    "ts-node": "^10.9.1",
    "vercel": "^34.1.7",
    "viem": "^2.9.16"
  },
  "devDependencies": {
    "@types/async": "^3.2.24",
    "@types/chai": "^4.3.4",
    "@types/jest": "29.5.12",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "eslint": "9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "tsc-alias": "1.8.8",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
